{
  "name": "云秒搭AI周报",
  "description": "Weekly AI Digest - 每周精选AI资讯",
  "build_command": "python3 deploy_to_edgeone.py",
  "output_directory": "public",
  "install_command": "pip install -r requirements.txt",
  "environment": {
    "PYTHON_VERSION": "3.9"
  },
  "routes": [
    {
      "src": "/articles/(.*)",
      "dest": "/articles/$1.html"
    },
    {
      "src": "/api/articles",
      "dest": "/posts/articles.json"
    }
  ]
}
